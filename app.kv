#:kivy 1.1.1
#:import hex kivy.utils.get_color_from_hex
#:import Svg kivy.graphics.svg.Svg

#:set board_prim "#795C34"
#:set board_sec "#E4D9CA"
<MainScreen>:
    MDBoxLayout:
        ChessBoard:

<ChessBoardSquare>:
    background_normal: ""
    background_down: ""
    image:image
    on_press: self.pressAction()
    Image:
        id:image
        size:self.parent.size
        pos:self.parent.pos
        source:''

<ChessBoard>:
    height: 0.8 * self.parent.parent.height
    width: self.height
    size_hint: None, None
    pos_hint: {"top":0.9}

MainScreen: